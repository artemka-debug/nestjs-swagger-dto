{
  "name": "nestjs-swagger-dto",
  "version": "0.0.0",
  "description": "Nestjs swagger dto decorators",
  "keywords": [
    "nestjs",
    "typescript",
    "swagger",
    "dto"
  ],
  "main": "dist/nestjs-swagger-dto.js",
  "types": "dist/nestjs-swagger-dto.d.ts",
  "files": [
    "dist"
  ],
  "author": "glebbash <glebbash@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/glebbash/nestjs-swagger-dto"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "lint": "eslint --fix 'src/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.build.json",
    "build:docs": "typedoc --out docs --theme minimal src",
    "test": "jest --coverage",
    "test:ci": "npm run lint && npm run test -- --no-cache"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "peerDependencies": {
    "@nestjs/swagger": ">= 4.8",
    "class-transformer": ">= 0.4",
    "class-validator": ">= 0.13"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@nestjs/common": "^7.6.17",
    "@nestjs/core": "^7.6.17",
    "@nestjs/swagger": "^4.8.0",
    "@types/jest": "26.0.19",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^5.1.3",
    "jest": "26.6.3",
    "jest-config": "26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.1.0",
    "semantic-release": "^17.4.0",
    "true-myth": "^4.1.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^10.4.0",
    "typedoc": "^0.20.28",
    "typescript": "^4.4.4"
  }
}
